{
  "name": "MinIO",
  "version": "0.2.0",
  "slug": "minio",
  "description": "MinIO object storage server",
  "url": "https://github.com/Delido/hassio-addons/blob/master/minio/README.md",
  "startup": "application",
  "boot": "auto",
  "webui": "http://[HOST]:[PORT:9001]",
  "arch": [
    "amd64",
    "aarch64"
  ],
  "ingress": true,
  "ingress_port": 80,
  "ingress_stream": true,
  "panel_icon": "mdi:store-24-hour",
  "panel_title": "MinIO",
  "hassio_api": false,
  "hassio_role": "default",
  "host_network": false,
  "homeassistant_api": false,
  "init": false,
  "privileged": [],
  "timeout": 90,
  "stage": "experimental",
  "map": [
    "share:rw"
  ],
  "ports": {
    "9001/tcp": 9001,
    "9000/tcp": 9000
  },
  "ports_description": {
    "9000/tcp": "MinIO API",
    "9001/tcp": "MinIO Console"
  },
  "options": {
    "timezone": "Europe/Berlin",
    "username": "minioadmin",
    "password": "minioadmin"
  },
  "schema": {
    "timezone": "str?",
    "username": "str?",
    "password": "str?"
  },
  "watchdog": "http://[HOST]:[PORT:9000]/minio/health/live"
}
